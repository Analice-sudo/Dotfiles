exec_always {
    gsettings set $gnome-schema gtk-theme ''
    gsettings set $gnome-schema icon-theme 'oomox-gruvbox-dark-medium'
    gsettings set $gnome-schema cursor-theme 'phinger-cursors-light'
    gsettings set $gnome-schema font-name 'Dina 10'
    seat seat0 xcursor_theme phinger-cursors-light 64
}
