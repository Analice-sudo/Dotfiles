#workspaces button {
	color: #9999a8;
	font-weight: Bold;
	background: #19191e;
	box-shadow: 0px 3px 0px 0px #222228;
	border: 2px solid #2b2b33;
	margin: 5px 5px 5px 3px;
	padding: 5px 0px 5px 0px;
	border-radius: 5px;
}

#workspaces button.focused {
	color: #9999a8;
	background: #19191e;
	border: 2px solid #2b2b33;
	box-shadow: 0px	3px 0px	0px #505a82;
}

#workspaces button.focused:hover {
	color: #9999a8;
	background: #19191e;
	border: 2px solid #373741;
	box-shadow: 0px	3px 0px	0px #727a9b;
	text-shadow: 0px 0px 0px rgba(0, 0, 0, 0);
	transition-timing-function: ease-out;
	transition-delay: 0ms;
	/* border-bottom: 2px solid #d79921; */
}

 #workspaces button.focused:active {
    color: #9999a8;
    background: #19191e;
    border: 2px solid #373741;
    box-shadow: 0px 2px 0px 0px #727a9b;
    text-shadow: 0px 0px 0px rgba(0, 0, 0, 0);
    margin: 7px 8px 5px 5px;
    animation: ease-out;
    transition: ease-out;
    /* border-bottom: 2px solid #d79921; */
}

#workspaces button:hover {
        color: #9999a8;
        background: #19191e;
		border: 2px solid #373741;
        box-shadow: 0px	3px 0px	0px #2b2b33;
        text-shadow: 0px 0px 0px rgba(0, 0, 0, 0);
	transition-timing-function: ease-out;
        transition-delay: 0ms;
	animation-duration: 0s;
}

